#!/usr/bin/env node

const { mediary } = require('../../..');
let data = {
    ids: [],
    map: {}
}

let cloned = mediary(data);
let i = 0;
while (global.times > i) {
    cloned.ids.push(i);
    cloned.map[i] = {
        a: 1,
        b: 'Some data here'
    };
    return cloned;
    i++;
}
data = cloned;
